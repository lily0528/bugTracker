@model TicketAssginDeveloper

@{
    ViewBag.Title = "AssignDeveloper";
}

<h2>AssignDeveloper</h2>
<form method="post">
    <div class="form-group">
        <div><h6>Ticket Id:</h6>@Model.TicketId</div>
        <div><h6>Ticket Title:</h6>@Model.TicketTitle</div>
    </div>
    <div class="form-group">
        @Html.LabelFor(Model => Model.DeveloperId, "Developer")
        @Html.DropDownListFor(Model => Model.DeveloperId, Model.DeveloperList, htmlAttributes: new { @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

